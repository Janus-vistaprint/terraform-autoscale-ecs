[
  {
    "cpu": ${cpu_value},
    "essential": true,
    "image": "${image_tag}",
    "memory": ${memory_hard},
    "memoryReservation": ${memory_soft},
    "name": "${container_name}",
    "environment": [
        {
            "name": "Environment",
            "value": "${task_environment}"
        }
    ],
    "portMappings": [
      {
        "containerPort": ${container_port},
        "hostPort": 0
      }
    ]
  }
]
